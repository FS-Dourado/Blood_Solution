const int lm35 = A1;
float temperatura;
void setup() {
  // put your setup code here, to run once:
  Serial.begin (9000);
}

void loop() {
  // put your main code here, to run repeatedly:
  int leitura = analogRead (lm35);
  temperatura = (5.0 / 1023) * leitura * 100;
  Serial.print ("Temperatura: ");
  Serial.println(temperatura);
  delay(2000);
}
